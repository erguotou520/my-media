{
  "name": "my-media-server",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "migrate": "bunx drizzle-kit migrate --config src/db/drizzle.config.ts",
    "migrate:js": "bunx drizzle-kit migrate --config dist/db/drizzle.config.js",
    "generate": "bunx drizzle-kit generate --config src/db/drizzle.config.ts",
    "seed": "bun run src/db/seed.ts",
    "seed:js": "bun run dist/db/seed.js",
    "dev": "bun run --watch src/index.ts",
    "build": "bun scripts/build.ts",
    "start": "bun run dist/index.js"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.23.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@elysiajs/bearer": "^1.1.1",
    "@elysiajs/jwt": "^1.1.0",
    "@elysiajs/static": "^1.1.0",
    "@elysiajs/swagger": "^1.1.0",
    "@libsql/client": "^0.7.0",
    "dayjs": "^1.11.12",
    "drizzle-orm": "^0.32.0",
    "elysia": "^1.1.3",
    "pino": "^9.3.1",
    "zod": "^3.23.8"
  }
}